{
  "name": "eslint-config-anvilabs",
  "version": "0.0.0-development",
  "description": "Anvilabs' ESLint config, following our internal styleguide",
  "keywords": [
    "eslint",
    "eslint config",
    "config",
    "anvilabs",
    "styleguide"
  ],
  "repository": "anvilabs/eslint-config-anvilabs",
  "homepage": "https://github.com/anvilabs/eslint-config-anvilabs#readme",
  "bugs": {
    "url": "https://github.com/anvilabs/eslint-config-anvilabs/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "files": [
    "rules/*.js",
    "flowtype.js",
    "index.js",
    "jest.js",
    "lodash.js",
    "react-base.js",
    "react-native.js",
    "react.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "find-new-rules:base": "eslint-find-rules --unused ./index.js",
    "find-new-rules:flowtype": "eslint-find-rules --unused ./test/fixtures/flowtype.js",
    "find-new-rules:jest": "eslint-find-rules --unused ./test/fixtures/jest.js",
    "find-new-rules:lodash": "eslint-find-rules --unused ./test/fixtures/lodash.js",
    "find-new-rules:react-native": "eslint-find-rules --unused ./test/fixtures/react-native.js",
    "find-new-rules:react": "eslint-find-rules --unused ./test/fixtures/react.js",
    "find-new-rules": "run-p --silent find-new-rules:*",
    "prettier-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "jest": "jest",
    "test": "run-p --silent lint find-new-rules prettier-check jest",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test",
    "push:watch": "git push origin master && travis-watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "jest": {
    "roots": [
      "<rootDir>/rules"
    ],
    "testEnvironment": "node",
    "testRegex": "/__tests__/.+-test\\.js$"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72
    }
  },
  "peerDependencies": {
    "eslint": "^3.19.0",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-eslint-comments": "^1.0.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-native": "^2.3.2",
    "eslint-plugin-unicorn": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-find-rules": "^3.1.1",
    "husky": "^0.14.2",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.2",
    "semantic-release": "^6.3.6",
    "travis-watch": "^1.17.1",
    "validate-commit-msg": "^2.12.2"
  }
}
