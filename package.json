{
  "name": "eslint-config-anvilabs",
  "version": "6.2.1",
  "description": "Anvilabs' ESLint config, following our internal styleguide",
  "keywords": [
    "eslint",
    "eslint config",
    "config",
    "anvilabs",
    "styleguide"
  ],
  "repository": "anvilabs/eslint-config-anvilabs",
  "homepage": "https://github.com/anvilabs/eslint-config-anvilabs#readme",
  "bugs": {
    "url": "https://github.com/kentcdodds/eslint-config-kentcdodds/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "files": [
    "flowtype.js",
    "index.js",
    "jest.js",
    "lodash.js",
    "react-base.js",
    "react-native.js",
    "react.js"
  ],
  "scripts": {
    "publish-github": "git push --follow-tags --no-verify && env-cmd ./.env.github-releaser conventional-github-releaser -p angular",
    "release": "standard-version",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "test": "npm-run-all --parallel lint find-new-rules prettier-check",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./index.js",
    "find-new-rules:flowtype": "eslint-find-rules --unused ./test/fixtures/flowtype.js",
    "find-new-rules:jest": "eslint-find-rules --unused ./test/fixtures/jest.js",
    "find-new-rules:lodash": "eslint-find-rules --unused ./test/fixtures/lodash.js",
    "find-new-rules:react-native": "eslint-find-rules --unused ./test/fixtures/react-native.js",
    "find-new-rules:react": "eslint-find-rules --unused ./test/fixtures/react.js",
    "prettier-check": "eslint --print-config index.js | eslint-config-prettier-check"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72
    }
  },
  "peerDependencies": {
    "eslint": "^3.17.1",
    "prettier": "^0.22.0"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-eslint-comments": "^1.0.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-react-native": "^2.3.1",
    "eslint-plugin-unicorn": "^2.1.0"
  },
  "devDependencies": {
    "conventional-github-releaser": "^1.1.3",
    "env-cmd": "^5.0.0",
    "eslint": "^3.17.1",
    "eslint-find-rules": "^1.14.3",
    "husky": "^0.13.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^0.22.0",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.11.2"
  }
}
